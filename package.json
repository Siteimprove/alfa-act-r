{
  "$schema": "https://json.schemastore.org/package",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test": "ava \"**/automated*\" \"**/assisted*\"",
    "strict": "ava \"**/automated*\" \"**/assisted*\" -- --strict",
    "testAssisted": "ava \"**/assisted*\"",
    "strictAssisted": "ava \"**/assisted*\" -- --strict",
    "testAutomated": "ava \"**/automated*\"",
    "strictAutomated": "ava \"**/automated*\" -- --strict",
    "fixtures": "node scripts/scrape.js"
  },
  "dependencies": {
    "@siteimprove/alfa-act": "^0.63.3",
    "@siteimprove/alfa-array": "^0.63.3",
    "@siteimprove/alfa-css": "^0.63.3",
    "@siteimprove/alfa-device": "^0.63.3",
    "@siteimprove/alfa-dom": "^0.63.3",
    "@siteimprove/alfa-future": "^0.63.3",
    "@siteimprove/alfa-hash": "^0.63.3",
    "@siteimprove/alfa-http": "^0.63.3",
    "@siteimprove/alfa-iterable": "^0.63.3",
    "@siteimprove/alfa-json-ld": "^0.63.3",
    "@siteimprove/alfa-map": "^0.63.3",
    "@siteimprove/alfa-option": "^0.63.3",
    "@siteimprove/alfa-rules": "^0.63.3",
    "@siteimprove/alfa-scraper": "^0.62.8",
    "@siteimprove/alfa-set": "^0.63.3",
    "@siteimprove/alfa-web": "^0.63.3",
    "jsonld": "^8.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.0.0",
    "@types/jsonld": "^1.5.8",
    "@types/node": "^18.11.10",
    "ava": "^5.1.0",
    "axios": "^1.2.0",
    "jsdom": "^22.0.0",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.0.0"
  },
  "ava": {
    "verbose": true,
    "environmentVariables": {
      "NODE_OPTIONS": ""
    },
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "packageManager": "yarn@3.6.0"
}
